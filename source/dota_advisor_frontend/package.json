{
  "name": "explorerfrontend",
  "version": "1.0.0-SNAPSHOT",
  "description": "Implementation of Dashboard with Multiple widgets",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:dev": "NODE_ENV=dev webpack --env=dev --progress --profile --colors",
    "build:prod": "NODE_ENV=production webpack --env=prod --progress --profile --colors",
    "build": "npm run build:prod",
    "start": "NODE_ENV=production webpack --host 0.0.0.0 --env=prod",
    "dev": "webpack-dev-server --content-base src --inline --hot --port 8085"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
